using System;

class Program
{
    static void Main()
    {
        Console.Write("Anna ensimm√§inen luku: ");
        int a = int.Parse(Console.ReadLine());

        Console.Write("Anna toinen luku: ");
        int b = int.Parse(Console.ReadLine());

